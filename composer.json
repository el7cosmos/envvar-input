{
    "name": "el7cosmos/envvar-input",
    "description": "Symfony console input with environment variable fallback",
    "license": "MIT",
    "authors": [
        {
            "name": "Luhur Abdi Rizal",
            "email": "el@elabee.me"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.2.5",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.65",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "EnvVarInput\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
